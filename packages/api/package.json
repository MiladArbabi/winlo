{
  "name": "api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "TS_NODE_TRANSPILE_ONLY=true nodemon --watch src --ext ts,js,json --exec \"node --loader ts-node/esm src/index.ts\"",
    "build": "tsc -p tsconfig.json && cp src/openapi.yaml dist/openapi.yaml",
    "migrate": "knex migrate:latest --knexfile ./config/knexfile.cjs",
    "seed": "knex seed:run --knexfile ./config/knexfile.cjs",
    "migrate:test": "knex migrate:latest --env test --knexfile ./config/knexfile.cjs",
    "seed:test": "knex seed:run     --env test --knexfile ./config/knexfile.cjs",
    "start": "node dist/index.js",
    "lint": "echo \"No lint configured yet\"",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --config ./config/jest.config.cjs", 
    "coverage": "npm test -- --coverage",
    "audit:ci": "npm audit --audit-level=moderate"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "express-pino-logger": "^7.0.0",
    "express-rate-limit": "^6.11.2",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "morgan": "^1.10.0",
    "pg": "^8.16.3",
    "pino": "^9.7.0",
    "pino-http": "^10.5.0",
    "pino-pretty": "^13.0.0",
    "redis": "^5.6.0",
    "swagger-ui-express": "^5.0.1",
    "yamljs": "^0.3.0",
    "zod": "^4.0.5"
  },
  "overrides": {
    "form-data": "^4.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-typescript": "^7.27.1",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@jest/globals": "^30.0.4",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/knex": "^0.15.2",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.0.13",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/yamljs": "^0.2.34",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  }
